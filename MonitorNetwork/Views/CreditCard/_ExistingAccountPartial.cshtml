@model MonitorNetwork.Models.CreditCardAndAccountViewModel

<div class="form-group">
    @Html.Label("Account Holder", htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownList("accountID", null, htmlAttributes: new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.creditcard.accountID, "", new { @class = "text-danger" })
    </div>
</div>

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="Create" class="btn btn-default" />
    </div>
</div>